			//function projectWorkComplete(){
			//	confirmBox("완료처리 하시겠습니까?", ajaxProjectWorkComplete)
			//}

			function ajaxProjectWorkComplete(){
				$.ajax({
					url : "${contextPath}/mng/req/project/projectWorkComplete",
					dataType : "JSON",
					type : "POST",
					data : {
						"reqProjectSeq": "${reqProjectSeq}"
						, "closeDescription": $("#closeDescription").val()
						,"stepLevel" 			 : "8"
						,"requestSeq" 			 : $("#allOrderNo").val()
						, "projectAlias": "${getProjectView.projectAlias}"
						, "projectManagerId": "${getProjectView.projectManagerId}"
						, "receiverId": "${userVo.userId}"		// 신청자 유저
						, "receiverName": "${userVo.userName}"  // 신청자 유저
						, "receiverMail": "${userVo.userMail}"	// 신청자 유저
						, "regUserId"	: "${userId}"	// 로그인 유저
						, "modUserId"	: "${userId}"	// 로그인 유저
						,"apprvUserId" 			 : "${userId}"
					},
					success : function(data) {
						//alertBox("완료처리 되었습니다.",workComplate);

						workComplate();
						//location.href = "${contextPath}/mng/oss/project/";
					},
					error : function(request, status, error) {
						//if(request.status=="200"){
						//	alertBox("재로그인  필요.",actReqProjectUpdate);
						//}
						alertBox3("code : " + request.status + "\n" + "error : " + request.responseText);
					}
				});
			}

			function workComplate(){
				location.href = "${contextPath}/mng/project/info/";
			}

			function valchk(){
				if($("#projectDescription").val()==""){
					alertBox3("작업처리내역을 입력하세요");
					$("#projectDescription").focus();
					return false;
				}
				return true;
			}

			// 체크박스 값 세팅
			function setNetworkVal(){
				$("input:checkbox[name='network']").each(function(){
					if($(this).val()=="Y"){
						$(this).attr("checked",true);
					}
				});
			}

			function moveListPage(){
				location.href="${contextPath}/mng/req/project/workApprovalProject/";
			}

			// 유효성 체크
			function validationChk(){
				if($("#oss_projectName").val() == ""){
					alertBox3("프로젝트명을 입력하세요.");
					$("#oss_projectName").focus();
					return false;
				}

				var ossProjectAlias = $("#oss_projectAlias").text();
				if(ossProjectAlias.replace(/ /gi, "") == ""){
					alertBox3("프로젝트 별칭을 입력하세요.");
					$("#oss_projectAlias").focus();
					return false;
				}
				return true;
			}

			function waiting(){
				alertBox("등록중입니다.</br>잠시만 기다려 주십시오.", waiting);
			}
			// 프로젝트 등록
			function actInsert() {
				
				
				
				alertBox("등록중입니다.<br>잠시만 기다려주십시오.", waiting);
				
				if(validationChk()){

					if($("#reqProjectSeq").val() == ""){
						alertBox3("프로젝트명을 입력하세요.");
						return false;
					}

					$.ajax({
						url : "${contextPath}/mng/oss/project/insert",
						dataType : "JSON",
						type : "POST",
						data : {
							"regUserId" 				: "${getProjectView.regUserId}"
							, "modUserId" 				: "${userId}"
							, "customerId" 				: "${getProjectView.customerId}"
							, "projectManagerId"		: "${getProjectView.projectManagerId}"
							, "projectName" 			: strTrim($("#oss_projectName").val())
							, "projectAlias" 			: strTrim($("#oss_projectAlias").text())
							, "description" 			: $("#oss_description").val()
						 	, "projectDeptCode"			: $("#projectDeptCode").val()
							, "projectPrice"			: $("#projectPrice").val()
							, "projectStartDatetime"	: $("#projectStartDatetime").val().replace(/-/gi, "")
							, "projectEndDatetime"		: $("#projectEndDatetime").val().replace(/-/gi, "")
							, "reqProjectSeq"			: $("#reqProjectSeq").val()
							, "cloudId"			: $("#cloudId").val()
							


						},
						success : function(data) {

							if(data.error == 1){

			 					$("#h_projectId").val(data.projectId);
			 					$("#h_projectName").val(data.projectName);
			 					reqProjectUpdate(strTrim($("#oss_projectName").val()));

							}else{
								alertBox3("등록실패");
							}
						},
						error : function(request, status, error) {
							alertBox3("code : " + request.status + "\n" + "error : " + request.responseText);
						}
					});
				}
			}

			//프로젝트 업데이트
			//수정자 : 홍승민 (smhong)
			//수정날짜 : 2021. 06. 27
			function reqProjectUpdate(projectName){
				var passInfoCheckBox = $( "#quota").prop("checked");
				var projectPass = passInfoCheckBox ? "Y" : "N";
				var _data = {
					"regUserId" 		: "${userId}"
					, "modUserId" 		: "${userId}"
					, "projectName" 	: projectName
					, "reqProjectSeq"	: $("#reqProjectSeq").val()
					, "projectAlias" 	: strTrim($("#oss_projectAlias").text())
				};
				
			 	if( projectPass == "Y"){
			 		var lollType = $( "#lollType").val();
			 		var memLimit = $( "#memLimit").val();
			 		var memReq = $( "#memReq").val();
			 		if( lollType == "now"){
			 			memLimit = memLimit + $( "#memLimit_type").val();
			 			memReq = memReq + $( "#memReq_type").val();
			 		}
			 		var extendExVar = {
						 "cpuLimit" : $( "#cpuLimit").val()
						, "memLimit" : memLimit
						, "cpuReq" : $( "#cpuReq").val()
						, "memReq" : memReq
						, "volumeReq" : $( "#volumeReq").val()
						, "podReq" : $( "#podReq").val()
						
					}
					$.extend( _data, extendExVar);
				} 
				
				$.ajax({
					url : "${contextPath}/mng/req/project/updateProject",
					dataType : "JSON",
					type : "POST",
					data : _data,
					success : function(data) {
		 				if(data == 1){
		 					//confirmBox2("프로젝트 등록이 완료되었습니다.",actReqProjectUpdate);
		 					alertBox("프로젝트 등록이 </br>완료되었습니다.<br>처리 내용이 신청 담당자에게<br>메일, SMS를 통해<br>전달되었습니다.",ajaxProjectWorkComplete);
						}else{
							alertBox3("등록실패");
							
						}
					},
					error : function(request, status, error) {
						alertBox3("code : " + request.status + "\n" + "error : " + request.responseText);
					}
				}); 
			}

			function moveDetailPage(){
				var params = "?projectId="+$("#h_projectId").val();
				params += "&projectName="+$("#h_projectName").val();
				location.href = "${contextPath}/mng/oss/project/view"+params;
			}

			//프로젝트 영문명 중복체크
			function projectNameChk(){

				if($("#oss_projectName").val()==""){
					alertBox3("프로젝트 ID를 입력하세요");
					return;
				}

				/* if( $("#oss_projectName").val().length != 7){
					alertBox3("프로젝트 ID 입력은 <br/>7자리를 입력하세요");
					return;
				} */
				$.ajax({
					url : "${contextPath}/mng/oss/project/",
					data : {
						"projectName" : $("#oss_projectName").val()
					},
					success : function(data) {
						var chkVal = data+1;

							$("#confirmMsg").css("color","blue");
							$("#confirmFlag").val("Y");

							var count = $("#hidden_clickCount").val();
							count++;
							$("#hidden_clickCount").val(count);

							if(count > 1){
								if($("#hidden_projectId").val() != $("#oss_projectName").val()){
								var projectVal = $("#oss_projectName").val();

								/* $("#oss_projectName").val(projectVal+chkVal); */
								$("#hidden_projectId").val($("#oss_projectName").val());

								projectNameNextChk();
								}
							}else{
	/* 							$("#oss_projectName").val($("#oss_projectName").val()+chkVal);
	 */							$("#hidden_projectId").val($("#oss_projectName").val());
								projectNameNextChk();
							}

					},
					error : function(request, status, error) {
						alertBox3("code : " + request.status + "\n" + "error : " + request.responseText);
					}
				});
			}

			//프로젝트 영문명 중복체크
			function projectNameNextChk(){

				if($("#oss_projectName").val()==""){
					alertBox3("프로젝트 ID를 입력하세요");
					return;
				}
				
				//프로젝트 특수문자 체크
				//만든이 smhong ( 홍승민)
				//2021. 06. 27
				var check_text = /^[A-Z0-9+]*$/;
				var check_spc = /[-_]/; // 특수문자

				if( !check_spc.test( $( "#oss_projectName").val()) && !check_text.test( $( "#oss_projectName").val())){
					alertBox3("프로젝트 ID는 영문[대문자]와 '-', '_'를 제외한 이름이 들어갈 수 없습니다");
					return;
				}
				//프로젝트 특수문자 체크 끝
				
				
				$.ajax({
					url : "${contextPath}/mng/oss/project/projectNameNextChk",
					data : {
						"projectName" : $("#oss_projectName").val()
					},
					success : function(data) {
						var chkVal = data;
						if(chkVal==0){
							$("#confirmMsg").css("color","blue");
							$("#confirmFlag").val("Y");
							//alertBox3("사용가능한 프로젝트 영문명</br>입니다.");
							if( $("#description").val() == "" ) {
								confirmBox("사용가능한 프로젝트명 입니다.</br>프로젝트 설명 없이 프로젝트명을 저장하시겠습니까?", saveProjectName);
							} else {
								confirmBox("사용가능한 프로젝트명 입니다.</br>프로젝트명을 저장하시겠습니까?", saveProjectName);
							}

						}else{
							$("#confirmMsg").css("color","#ff0000");
							$("#confirmFlag").val("N");
							alertBox3("사용할 수 없는 프로젝트 </br>영문명 입니다.");
						}
					},
					error : function(request, status, error) {
						alertBox3("code : " + request.status + "\n" + "error : " + request.responseText);
					}
				});
			}

			function saveProjectName() {
				
				$.ajax({
					url : "${contextPath}/mng/req/project/updateProject",
					data : {
						"reqProjectSeq" : "${reqProjectSeq}",
						"projectName" : $("#oss_projectName").val(),
						"reqProjectDescription" : $("#description").val(),
						"modUserId" : "${userId}"
					},
					success : function(data) {
						var chkVal = data;
						if(chkVal==0){
							$("#confirmMsg").css("color","blue");
							$("#confirmFlag").val("N");
							alertBox3("저장이 실패하였습니다.");
						}else{
							$("#confirmMsg").css("color","#ff0000");
							$("#confirmFlag").val("Y");
							alertBox3("저장되었습니다.");
						}
					},
					error : function(request, status, error) {
						alertBox3("code : " + request.status + "\n" + "error : " + request.responseText);
					}
				});
			}


			// 팝업메시지 세팅
	// 		function alertModal(txt){
	// 			$("#confirmMsg").text(txt);
	// 			$("#message-modal-dialog").modal();
	// 		}

			// 팝업메시지 세팅
			function alertModal(txt, fn){
				$("#confirmMsg").text(txt);
				$("#setBtn").html("");

				if(fn==""||fn=="undefined"){
					var vHtml = "";
					vHtml = '<a type="button" class="btn width-100 btn-grey" data-dismiss="modal">닫기</a>';
					$("#setBtn").html(vHtml);
				}else{
					var vHtml = "";
					vHtml = '<a type="button" class="btn width-100 btn-grey" data-dismiss="modal" onclick="'+fn+'">닫기</a>';
					$("#setBtn").html(vHtml);
				}

				$("#message-modal-dialog").modal({backdrop:'static'});
			}

			function strTrim(str){
				return str.replace(/(^\s*)|(\s*$)/g, "");
			}

			//승인 버튼 누를시 체크 박스 확인
			function projectApprving(stepLevel,requestSeq,rightStepId){

			    if($("#confirmFlag").val() == "N" && (stepLevel == "5" || stepLevel == "6") ){
					alertBox3("프로젝트명 중복을 확인하세요.");
					return false;
				}

				var jsonData = {
						"requestSeq" 			 : $("#allOrderNo").val()
						,"stepLevel" 			 : stepLevel
						,"rightStepId"			 : rightStepId
						,"apprvDescription"		 : $("#apprvDescription").val()
						,"orderUserId" 			 : "${userVo.userId}"
						,"apprvUserName" 		 : "${userName}"
						,"apprvUserId" 			 : "${userId}"
						,"reqProjectDescription" : $("#description").val()
						,"reqProjectName" 		 : $("#oss_projectName").val()
					};

				if($("input:checkbox[name='projectIdchk']:checked").length == $("input:checkbox[name='projectIdchk']").length){
					confirmBox2("승인 처리  하시겠습니까?", checkWorkUser ,jsonData);

				}else if($("input:checkbox[name='projectIdchk']:checked").length==0 || $("input:checkbox[name='projectIdchk']:checked").length != $("input:checkbox[name='projectIdchk']").length){
	// 				alertBox3("체크박스를 확인해 주세요");
					alertBox3("체크박스를 확인해 주세요");
				}
			}

			//승인 버튼 누를시 체크박스 후 작업자지정 확인
			function checkWorkUser(jsonData){
				$.ajax({
					url : "${contextPath}/mng/req/product/checkWorkUser",
					dataType : "JSON",
					type : "POST",
					data : {
						"orderNo" : $("#allOrderNo").val()
					},
					success : function(data) {
						var result = data
						if(result > 0){
							alertBox3("작업자를 지정해 주세요.");
						}else{
							ajaxProjectApprving(jsonData);
						}
					},
					error : function(request, status, error) {
						alertBox3("code:" + request.status + "\n" + "error:" + error);
					}
				});
			}

			function ajaxProjectApprving(jsonObj){
				if(jsonObj.rightStepId != '*'){

					$.ajax({
						url : "${contextPath}/mng/req/project/stepUpdate",
						dataType : "JSON",
						type : "POST",
						data : jsonObj,
						success : function(data) {
							var result = data;
							if(result==1){
								alertBox("승인 처리가 완료 되었습니다.<br>처리 내용이 신청 담당자에게<br>메일, SMS를 통해<br>전달되었습니다.", moveListPage);
							}else{
								alertBox("fail!!", moveListPage);
							}
						},
						error : function(request, status, error) {
							alertBox3("code:" + request.status + "\n" + "error:" + error);
						}
					});
				}else if(jsonObj.rightStepId == '*'){
					
				
					
						alertBox("처리중 입니다.<br>잠시만 기다려 주십시오.", waiting);
	// 					var jsonData = setJsonData();
						$.ajax({
	                        //최종 승인후 추가 로직 필요시 개발
							//	url : "${contextPath}/mng/req/product/insert",
						    url : "${contextPath}/mng/req/project/stepUpdate",
							dataType : "JSON",
							type : "POST",
							data : jsonObj,
	// 							{
	// 								"requestSeq" : $("#allOrderNo").val()
	// 								, "onDemandFlag" : $("#onDemandFlag").val()
	// 								,"orderUserId" : "${userVO.userId}"
	// 							},
							success : function(data) {
								var result = data;
								if(result=1){

								actInsert();

								}else{
									alertBox("fail!!", moveListPage);
								}
							},
							error : function(request, status, error) {
								alertBox3("code:" + request.status + "\n" + "error:" + error);
							}
						});
				}
			}


			var rejectStepLevel = "";
			var rejectOrderNo = "";
			var rejectRightStepId = "";


			//미승인 버튼 누를시
			function projectRejectedPopup(stepLevel,requestSeq,rightStepId) {
				$("#modal-dialog-reject").modal();
				rejectStepLevel = stepLevel;
				rejectOrderNo = $("#allOrderNo").val();
				rejectRightStepId = rightStepId;
			}

			function projectRejected() {

				if($("#apprvDescription").val()==""){
					$("#apprvDescription").focus();
					alertBox("프로젝트 반려시 반려사유를<br>입력해 주시기 바랍니다.");
					return false;
				}

				$.ajax({
					url : "${contextPath}/mng/req/project/rejectedUpdate",
					dataType : "JSON",
		//			type : "POST",
					data : {
						"requestSeq" : rejectOrderNo
						,"stepLevel" : rejectStepLevel
						,"apprvDescription":$("#apprvDescription").val()
						,"orderUserId" : "${getOrderUserId.orderUserId}"
						,"apprvUserId" : "${userId}"
						,"apprvUserName" : "${userName}"
					},
					success : function(data) {
						var result = data;
						if(result==1){
							alertBox("프로젝트신청이 반려되었습니다.", moveListPage)
						}else{
							alertBox3("fail!!");
							moveListPage();
						}
					},
					error : function(request, status, error) {
						alertBox3("code:" + request.status + "\n" + "error:" + error);
					}
				});
			}

			// 신청 버튼 클릭시 결재자 지정 팝업 Open
			function openApproveReviewModal(projectStatusFlag) {
				//01 작성중,02 처리중,03 반려,04 처리완료
				if($("#quota").is(":checked") == true){
					if($("#cpuReq").val()*1 > $("#cpuLimit").val()*1){
						alertBox3("CPU사용량 설정을 확인해 주세요.");
						return false;
					}
					if($("#memReq").val()*1 > $("#memLimit").val()*1){
						alertBox3("Memory사용량 설정을 확인해 주세요.");
						return false;
					}
					
					//LIMIT CHECK
					var lollType = $( "#lollType").val(); //신청시에만
					if( lollType == "now"){
						if( $( "#memReq_type").val() == ""){
							alertBox3("Memory사용량 타입 설정을 선택해 주세요.");
							return false;
						}
						
						if( $( "#memLimit_type").val() == ""){
							alertBox3("Memory사용량 타입 설정을 선택해 주세요.");
							return false;
						}
						
						if( $( "#memReq_type").val() == "Mi" && $( "#memLimit_type").val() == "Gi"){
							alertBox3("Memory사용량 타입 설정을 확인해 주세요.");
							return false;
						}
						
					}
					
				}
				$("#h_projectStatusFlag").val(projectStatusFlag);
				$("#pop_projectAlias").text($("#projectAlias").val());
				$("#pop_description").text($("#projectDescription").val());
				if($("#oss_projectName").val()==""){
					alertBox3("프로젝트 ID를 입력하세요");
					return;
				}
				if($("#confirmFlag").val() == "N"){
					alertBox3("프로젝트명 중복을 확인하세요.");
					return false;
				}
				if(projectStatusFlag == 1) {
					projectInsert();
				} else {
					if($("#apprvPass").val() == "Y"){
						confirmBox4("결제자를 지정하시겠습니까?",approvSelect,nApprovSelect);
					} else {
						approvSelect();
					}
				}
			}

			function nApprovSelect(){
				
				updateProjectCloudQuota();
			//	updateProjectCloudQuotaNutanix();

				$("#pop_projectAlias").text($("#projectAlias").val());
				$("#pop_description").text($("#projectDescription").val());
				$("#pop_price").text($("#price").val());
				$("#pop_dispSdate").text($("#dispSdate").val());
				$("#pop_dispEdate").text($("#dispEdate").val());
				$("#pop_customerName").text($("#customerName").val());
				$("#pop_deptName").text($("#deptName").val());
				$("#selectApprv").val("Y");
				projectInsert();
			}
			function approvSelect(){
				$("#selectApprv").val("N");
				$("#modal-approve-review-dialog").modal();		
			}
			

			// 결재지정 담당자 검색
			function openReviewModal(e){
				currentApprovalLine = e;
				var tmpCurrentApprovalStep = currentApprovalLine.id.replace("approvalLine", "");
				if( approvalList[tmpCurrentApprovalStep-1] != undefined) {
					confirmBox("재신청할경우 현재 상위결재자값이 초기화 됩니다.", clearUpperApproval );

				} else {
					$("#user-modal-review-dialog").modal();
					gridPopList();
				}
			}

			// 승인부서 결재지정 팝업 - 담당자조회 검색
			function gridPopList(){
				$.ajax({
					url : "${contextPath}/mbr/operation/approveUser/addProjectApproveList",
					dataType : "JSON",
					data : {
						"searchKind": $("#reviewSearchKind option:selected").val()
						, "keyword"	: $("#reviewUserId").val()
						, "cloudId" : $("#cloudId").val()
					},
					success : function(req) {
						if(req.data.length == 0){
							$("#popListReviewTable").hide();
							$("#popListReviewTable2").show();
						}else{
							$("#popListReviewTable").empty();

							$.each(req.data, function(key,val){
								$("#popListReviewTable").append(
									'<tr>'
									    +'<td class="tac">'+val.upDeptName+'/'+val.deptName+'</td>'
										+'<td class="tac">'+val.userId+'</td>'
										+'<td class="tac">'+val.userLevel+'</span>'
										+'<td class="tac">'+val.userName+'</span>'
										+'<td class="tac">'+val.userConditionText+'</td>'
										+'<td class="tac"><input type="button"  value="&#xf00c"  class="popCheckbtn"  onclick="updateReview(this, \''+val.userId+'\', \''+val.userName+'\', \''+val.deptName+'\',  \''+val.userLevel+'\',  \''+val.userLevelCode+'\');" initModal();></td>'
									+'</tr>');

							});
							$("#popListReviewTable").show();
							$("#popListReviewTable2").hide();
						}
						totalCnt = req.data.length;
						$("#totalReviewCnt").text("총 "+totalCnt+" 건");
					},
					error : function(request, status, error) {
						if(request.status == '200'){
							alertBox("접속이 만료되었습니다.", logout);
						}else{
							alertBox3("code:" + request.status + "\n" + "error:" + error);
						}
					}
				});
			}

			function clearUpperApproval() {
				var tmpCurrentApprovalStep = currentApprovalLine.id.replace("approvalLine", "");
				for(var i = tmpCurrentApprovalStep-1 ; i < 4; i++) {
					approvalList.splice(i, 1);

					$("#approvalLine"+(i+1)+"_userId").text("사번");
					$("#approvalLine"+(i+1)+"_userLevel").text("직급");
					$("#approvalLine"+(i+1)+"_userName").text("이름");

					$("#approvalLine"+(i+1)).attr('disabled', true);
				}

				//초기화 하더라도 차장급 이상이면 재활성화되게
				if( ${userLevelCode} < 5 ) {
					$("#approvalLine2").attr('disabled', false);
				}

				$("#user-modal-review-dialog").modal();
				gridPopList();
			}

			//요청부서 검토자/승인자 지정 - 요청부서(검토) 검토자지정
			function updateReview(e, userId, userName, deptName, userLevel, userLevelCode) {
				var tmpCurrentApprovalStep = currentApprovalLine.id.replace("approvalLine", "");
				if( tmpCurrentApprovalStep > 1 && approvalList[tmpCurrentApprovalStep-2] != undefined) {
					if( approvalList[tmpCurrentApprovalStep-2].userLevelCode < userLevelCode) {
						alertBox("이전단계보다 같거나 높은 직급의 담당자를 선택하세요");
						return false;
					}
				}
				/* if($("#hidden_login_user_id").val() ==userId) {
					alertBox("신청자는 본인을<br>결재자로 지정할수 없습니다.");
					return false;
				} */

				$("#popListReviewTable > tr").find("input").removeClass("active");
				$(e).toggleClass("active");
				var tmpValue = { "userId" : userId, "deptName" : deptName, "userName" : userName, "userLevel" : userLevel, "userLevelCode" : userLevelCode }


				$("#"+currentApprovalLine.id+"_userId").text(userId);
				$("#"+currentApprovalLine.id+"_userLevel").text(userLevel);
				$("#"+currentApprovalLine.id+"_userName").text(userName);

				approvalList[tmpCurrentApprovalStep-1] = tmpValue;
				$("#hiddenUserIdReview").val(userId);
				$("#hiddenDeptNameReview").val(deptName);
				$("#hiddenUserNameReview").val(userName);

				// 다음 단계 버튼 활성화
				$("#approvalLine"+(parseInt(tmpCurrentApprovalStep)+1) ).attr('disabled', false);

			}

			// 결재라인 초기화
			function initApprovalLine() {
				var tmpUserLevelCode = 100;
				if( ${userLevelCode} == null || ${userLevelCode} == '' ) {
					tmpUserLevelCode == 100;
				} else {
					tmpUserLevelCode == ${userLevelCode};
				}

				// 직급에 따른 버튼활성화 - 3직급(차장) == 4
				$("#approvalLine1").attr('disabled', false);
				if( ${userLevelCode} < 5 ) {
					$("#approvalLine2").attr('disabled', false);
				}
			}

			function checkApproval() {
				if( approvalList[3] == undefined || approvalList[3] == null || approvalList[3] == '' ) {
					alertBox3("결재자를 지정해 주세요");
					return false;
				}

				$("#pop_projectAlias").text($("#projectAlias").val());
				$("#pop_description").text($("#projectDescription").val());
				$("#pop_price").text($("#price").val());
				$("#pop_dispSdate").text($("#dispSdate").val());
				$("#pop_dispEdate").text($("#dispEdate").val());
				$("#pop_customerName").text($("#customerName").val());
				$("#pop_deptName").text($("#deptName").val());

				projectInsert();
			}

			//데이터 세팅
			function setJsonData(){
				
				
				if( approvalList[0] == undefined || approvalList[0] == null) {
					var tmp = {"userId" : "", "deptName" : "", "userName" : "", "userLevel" : "", "userLevelCode" : ""};
					approvalList[0] = tmp;
				}
				var quotaYn = "";
				if($("#quota").is(":checked") == true){
					quotaYn = "Y";
				} else {
					quotaYn = "N";
				}
				
				var jsonData = {
						"reqProjectSeq": $("#reqProjectSeq").val()
						, "regUserId" : '${userId}'
						, "approvalStepList" : approvalList
						, "apprvPass" : $("#apprvPass").val()
						, "selectApprv" : $("#selectApprv").val()
						, "paas" : $("#paas").val()
						, "quotaYn" : quotaYn
						, "cpuLimit" : $( "#cpuLimit").val()
						, "memLimit" : ( $( "#memLimit").val() +  $( "#memLimit_type").val())
						, "cpuReq" : $( "#cpuReq").val()
						, "memReq" : ( $( "#memReq").val() + $( "#memReq_type").val())
						, "volumeReq" : $( "#volumeReq").val()
						, "podReq" : $( "#podReq").val()
						, "reqProjectCloudQuotaList" : reqProjectCloudQuotaList
						, "reqProjectCloudQuotaNutanixList" : reqProjectCloudQuotaNutanixList
						
					};
				return jsonData;
			}

			// 승인부서 결재라인 업데이트
			function projectInsert() {
				
				   updateProjectCloudQuota();
				  // updateProjectCloudQuotaNutanix();
				   
				var jsonData = setJsonData();

				$.ajax({
					url : "${wasPath}/mng/req/project/updateApprovalLine",
					dataType : "JSON",
					type : "POST",
					data : jsonData,

					success : function(data) {

						var result = data;
						var res = data.res;
						var apprv = data.apprv;
						var programSeq = data.reqProjectSeq;

						if(res==1){
							if(apprv == "pass"){
								actInsert();
							} else{
								$("#programSeq").val(programSeq);
								alertBox("저장하였습니다.", moveListPage);
							}
						} else {
							alertBox3("fail!");
						}

					},
					error : function(request, status, error) {
						if(request.status == '200'){
							alertBox("접속이 만료되었습니다.", logout);
						}else{
							alertBox3("code:" + request.status + "\n" + "error:" + error);
						}
					}
				});
			}

		
			
			
			$(document).ready(function() {
			    App.init();
			    initApprovalLine();
			/*     $("#quota").on("click" ,function(e){
					var slefThis = this;
					var checkbox_check = $( slefThis).prop("checked");
			
					$( "#quota_paas").hide();
					
					if( checkbox_check){
						$( "#quota_paas").show();
					}
				}); */
				

			    if("${getProjectView.reqProjectName}" != "" && "${getProjectView.reqProjectName}" != null){
			    	$("#confirmFlag").val("Y")
			    } else {
			    	$("#confirmFlag").val("N")
			    }

			  	//메뉴활성화
				$("#req").addClass("active");
				$("#projectManage").addClass("active");

				$("#description").val($("#req_description").val());

				/* 프로젝트 등록 부분 계속 노출 될 수 있도록
			    if("${getProjectView.projectId}" != "" && "${getProjectView.projectId}" != null){
			    	$("#divProject").hide();
			    } else {
			    	$("#divNetwork").hide();
			    }
				*/

				$("#oss_projectName").on({
					"keypress" : function() { //프로젝트영문명 입력시 중복체크 flag
						$("#confirmFlag").val("N");
					},
					"keyup blur" : function(e){
						var pattern = /[ㄱ-ㅎ|ㅏ-ㅣ|가-힣]/gi;
						if(pattern.test(this.value)){
							$(this).val(this.value.replace(pattern, ''));
						}
	
			 			var pattern2 = /[\{\}\[\]\/?.,;:|\)*~`!^+<>@\#$%&=\\\(\'\"]/gi;
						if(pattern2.test(this.value)){
							$(this).val(this.value.replace(pattern2, ''));
						}
						$(this).val(this.value.toUpperCase());
					}
				});

				$("a>button").on('click',function(){
					location.href = $(this).closest("a").attr("href");
				});

			// setNetworkVal();

			});

			$("#files").change(function(){
				confirmBox("파일을 추가하시겠습니까??", actFileUpload);
			})
			function actFileUpload(){
				$('#file-form').submit();
			}

			function onlyNum(e){
				var charCode = (e.which) ? e.which : event.keyCode
		         if (charCode > 31 && (charCode < 48 || charCode > 57)){
		            return false;
		         }
		         return true;
			}

			function updateProjectCloudQuota() {
				
				for(var i=0; i< '${getProjectCloudView.size()}';i++){
					
					var quotaYn = 'N';
					if($("#quota"+i+"").is(":checked") == true){
						quotaYn = 'Y';
						
						
						if($("#cpuReq"+i+"").val() == undefined || $("#cpuReq"+i+"").val() == null || $("#cpuReq"+i+"").val() == ''){
							alertBox3("cpuReq null");
							return false;							
						}
						if($("#cpuLimit"+i+"").val() == undefined || $("#cpuLimit"+i+"").val() == null || $("#cpuLimit"+i+"").val() == ''){
							alertBox3("cpuLimit null");
							return false;							
						}
						if($("#memReq"+i+"").val() == undefined || $("#memReq"+i+"").val() == null || $("#memReq"+i+"").val() == ''){
							alertBox3("memReq null");
							return false;							
						}
						if($("#memLimit"+i+"").val() == undefined || $("#memLimit"+i+"").val() == null || $("#memLimit"+i+"").val() == ''){
							alertBox3("memLimit null");
							return false;							
						}
						if($("#volumeReq"+i+"").val() == undefined || $("#volumeReq"+i+"").val() == null || $("#volumeReq"+i+"").val() == ''){
							alertBox3("volumeReq null");
							return false;							
						}
						if($("#podReq"+i+"").val() == undefined || $("#podReq"+i+"").val() == null || $("#podReq"+i+"").val() == ''){
							alertBox3("podReq null");
							return false;							
						}
						
						
						if($("#cpuReq").val()*1 > $("#cpuLimit").val()*1){
							alertBox3("CPU사용량 설정을 확인해 주세요.");
							return false;
						}
						
						if($("#cpuReq"+i+"").val()*1 > $("#cpuLimit"+i+"").val()*1){
							alertBox3("CPU사용량 설정을 확인해 주세요.");
							return false;
						}
						
						if($("#memReq"+i+"").val()*1 > $("#memLimit"+i+"").val()*1){
							alertBox3("Memory사용량 설정을 확인해 주세요.");
							return false;
						}
						
						if( $( "#memReq_type"+i+"").val() == ""){
							alertBox3("Memory사용량 타입 설정을 선택해 주세요.");
							return false;
						}
						
						if( $( "#memLimit_type"+i+"").val() == ""){
							alertBox3("Memory사용량 타입 설정을 선택해 주세요.");
							return false;
						}
						
						if( $( "#memReq_type"+i+"").val() !=  $( "#memLimit_type"+i+"").val()){
							alertBox3("Memory사용량 타입 설정을 확인해 주세요.");
							return false;
						}
						
						var quotaInfo = { 							
								"cloudId" : $("#cloudId"+i+"").val(), 
								"cpuReq" : $("#cpuReq"+i+"").val(), 
								"cpuLimit" : $("#cpuLimit"+i+"").val(),
								"memReq" : $("#memReq"+i+"").val()+$( "#memReq_type"+i+"").val(),
								"memLimit" : $("#memLimit"+i+"").val()+$( "#memLimit_type"+i+"").val(),
								"volumeReq" : $("#volumeReq"+i+"").val(),
								"podReq" : $("#podReq"+i+"").val(),
								"quotaYn" : quotaYn,
								
								"podCpuMin" : $("#podCpuMin"+i+"").val(), 
								"podCpuMax" : $("#podCpuMax"+i+"").val(),
								"podMemMin" : $("#podMemMin"+i+"").val()+$( "#podMemMin_type"+i+"").val(),
								"podMemMax" : $("#podMemMax"+i+"").val()+$( "#podMemMax_type"+i+"").val(),
								
								"limitRangeCpuMin" : $("#limitRangeCpuMin"+i+"").val(), 
								"limitRangeCpuMax" : $("#limitRangeCpuMax"+i+"").val(),
								"limitRangeMemMin" : $("#limitRangeMemMin"+i+"").val()+$( "#limitRangeMemMin_type"+i+"").val(),
								"limitRangeMemMax" : $("#limitRangeMemMax"+i+"").val()+$( "#limitRangeMemMax_type"+i+"").val(),
								"limitRangeCpuDefault" : $("#limitRangeCpuDefault"+i+"").val(), 
								"limitRangeMemDefault" : $("#limitRangeMemDefault"+i+"").val(),
								"limitRangeCpuDefaultRequest" : $("#limitRangeCpuDefaultRequest"+i+"").val(), 
								"limitRangeMemDefaultRequest" : $("#limitRangeMemDefaultRequest"+i+"").val(),
								"limitRangeCpuLimitRequestRatio" : $("#limitRangeCpuLimitRequestRatio"+i+"").val(), 
								"limitRangeMemLimitRequestRatio" : $("#limitRangeMemLimitRequestRatio"+i+"").val()
								
						}
						
					    reqProjectCloudQuotaList[i] = quotaInfo;
						
					}
				}	
			}
			
