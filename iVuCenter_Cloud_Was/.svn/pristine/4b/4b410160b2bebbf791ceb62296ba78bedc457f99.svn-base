<!-- 				, OM.mcaCnt AS mcaCnt -->
<!-- 				, OS.scmCnt AS scmCnt -->
<!-- 				, OSEC.secCnt AS secCnt -->
			FROM oss_project OP

			LEFT JOIN (
				SELECT
					project_id
					, COUNT(*) AS vmCnt
					, SUM(vcpus) AS vcpusCnt
					, SUM(mem_gb) AS memCnt
					, SUM(disk_gb) AS diskCnt
				FROM oss_vm
				WHERE del_flag = 'N'
				AND IFNULL(expire_datetime, '9999-12-31') >= NOW()
				GROUP BY project_id
			) OV
			ON OP.project_id = OV.project_id

			LEFT JOIN (
