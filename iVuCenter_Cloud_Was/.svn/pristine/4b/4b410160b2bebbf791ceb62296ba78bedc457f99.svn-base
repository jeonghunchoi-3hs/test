				FROM oss_nas
				WHERE del_flag = 'N'
				AND IFNULL(expire_datetime, '9999-12-31') >= NOW()
				GROUP BY project_id
			) ON2
			ON OP.project_id = ON2.project_id

			LEFT JOIN (
				SELECT
					project_id
					, COUNT(*) AS lbCnt
				FROM oss_loadbalancer
				WHERE del_flag = 'N'
				AND IFNULL(expire_datetime, '9999-12-31') >= NOW()
				GROUP BY project_id
			) OL
			ON OP.project_id = OL.project_id

			LEFT JOIN (
